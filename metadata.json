{
  "name": "markt-artifactory",
  "version": "3.0.5",
  "author": "markt.de",
  "summary": "Install and configure JFrog Artifactory",
  "license": "Apache-2.0",
  "source": "https://github.com/markt-de/puppet-artifactory",
